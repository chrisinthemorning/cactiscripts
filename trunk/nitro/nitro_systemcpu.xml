<interface>
  <name>Get Netscaler CPU Information</name>
  <script_path>/usr/bin/perl |path_cacti|/scripts/nitro_query_ns.pl</script_path>
  <arg_prepend>|host_hostname| soap_stats 37GvYs12t5 systemcpu</arg_prepend>
  <arg_index>index id</arg_index>
  <arg_query>query id</arg_query>
  <arg_get>get id</arg_get>
  <arg_num_indexes>num_indexes</arg_num_indexes>
  <output_delimeter>!</output_delimeter>
  <fields>
    <cpuid>
      <name>id</name>
      <direction>input</direction>
      <query_name>id</query_name>
    </cpuid>
    <percpuuse>
      <name>percpuuse</name>
      <direction>output</direction>
      <query_name>percpuuse</query_name>
    </percpuuse>
  </fields>
</interface>
