<interface>
  <name>Get Netscaler CS Vserver Information</name>
  <script_path>/usr/bin/perl |path_cacti|/scripts/nitro_query_ns.pl</script_path>
  <arg_prepend>|host_hostname| soap_stats 37GvYs12t5 csvserver</arg_prepend>
  <arg_index>index name</arg_index>
  <arg_query>query name</arg_query>
  <arg_get>get name</arg_get>
  <arg_num_indexes>num_indexes</arg_num_indexes>
  <output_delimeter>!</output_delimeter>
  <index_order>csname</index_order>
  <fields>
    <csname>
      <name>csvservername</name>
      <direction>input</direction>
      <query_name>name</query_name>
    </csname>
    <type>
      <name>type</name>
      <direction>input</direction>
      <query_name>type</query_name>
    </type>
    <primaryipaddress>
      <name>primaryipaddress</name>
      <direction>input</direction>
      <query_name>primaryipaddress</query_name>
    </primaryipaddress>
    <state>
      <name>state</name>
      <direction>input</direction>
      <query_name>state</query_name>
    </state>
    <labelledconn>
      <name>labelledconn</name>
      <direction>output</direction>
      <query_name>labelledconn</query_name>
    </labelledconn>
    <totalrequests>
      <name>totalrequests</name>
      <direction>output</direction>
      <query_name>totalrequests</query_name>
    </totalrequests>
    <sothreshold>
      <name>sothreshold</name>
      <direction>output</direction>
      <query_name>sothreshold</query_name>
    </sothreshold>
    <responsebytesrate>
      <name>responsebytesrate</name>
      <direction>output</direction>
      <query_name>responsebytesrate</query_name>
    </responsebytesrate>
    <totalresponses>
      <name>totalresponses</name>
      <direction>output</direction>
      <query_name>totalresponses</query_name>
    </totalresponses>
    <requestbytesrate>
      <name>requestbytesrate</name>
      <direction>output</direction>
      <query_name>requestbytesrate</query_name>
    </requestbytesrate>
    <pushlabel>
      <name>pushlabel</name>
      <direction>output</direction>
      <query_name>pushlabel</query_name>
    </pushlabel>
    <responsesrate>
      <name>responsesrate</name>
      <direction>output</direction>
      <query_name>responsesrate</query_name>
    </responsesrate>
    <deferredreq>
      <name>deferredreq</name>
      <direction>output</direction>
      <query_name>deferredreq</query_name>
    </deferredreq>
    <totspillovers>
      <name>totspillovers</name>
      <direction>output</direction>
      <query_name>totspillovers</query_name>
    </totspillovers>
    <totalrequestbytes>
      <name>totalrequestbytes</name>
      <direction>output</direction>
      <query_name>totalrequestbytes</query_name>
    </totalrequestbytes>
    <deferredreqrate>
      <name>deferredreqrate</name>
      <direction>output</direction>
      <query_name>deferredreqrate</query_name>
    </deferredreqrate>
    <totalresponsebytes>
      <name>totalresponsebytes</name>
      <direction>output</direction>
      <query_name>totalresponsebytes</query_name>
    </totalresponsebytes>
    <primaryport>
      <name>primaryport</name>
      <direction>output</direction>
      <query_name>primaryport</query_name>
    </primaryport>
    <requestsrate>
      <name>requestsrate</name>
      <direction>output</direction>
      <query_name>requestsrate</query_name>
    </requestsrate>
  </fields>
</interface>
